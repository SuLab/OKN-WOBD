# List 10 NDE Datasets
# Endpoint: https://frink.apps.renci.org/nde/sparql

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX schema: <http://schema.org/>

SELECT ?dataset ?name ?description ?species
WHERE {
  ?dataset rdf:type schema:Dataset .
  OPTIONAL { ?dataset schema:name ?name }
  OPTIONAL { ?dataset schema:description ?description }
  OPTIONAL { ?dataset schema:species ?species }
}
LIMIT 10
