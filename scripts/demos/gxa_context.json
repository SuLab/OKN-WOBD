  "example_queries": {
    "find_studies_for_disease": {
      "description": "Find studies investigating a specific disease",
      "sparql": "SELECT ?study ?title ?description WHERE {\n  ?disease a biolink:Disease ;\n           biolink:name ?diseaseName .\n  FILTER(CONTAINS(LCASE(?diseaseName), 'breast cancer'))\n  ?study biolink:studies ?disease ;\n         spokegenelab:project_title ?title ;\n         biolink:description ?description .\n}\nLIMIT 20"
    },
    "go_enrichment_for_term": {
      "description": "Find GO term enrichments across studies",
      "sparql": "SELECT ?study ?studyTitle ?assay ?pvalue ?goTermName\nWHERE {\n  ?enrichment biolink:participates_in ?goTerm ;\n              spokegenelab:adj_p_value ?pvalue .\n  ?goTerm biolink:name ?goId .\n  FILTER(?goId = 'GO:0006955')  # immune response\n  OPTIONAL { ?goTerm biolink:id ?goTermName }\n  ?assay biolink:has_output ?enrichment .\n  ?study biolink:has_output ?assay ;\n         biolink:name ?studyTitle .\n}\nORDER BY ?pvalue\nLIMIT 50"
    },
    "studies_by_perturbation": {
      "description": "Find studies using a specific experimental factor",
      "sparql": "SELECT ?study ?title ?factors WHERE {\n  ?study a biolink:Study ;\n         spokegenelab:project_title ?title ;\n         spokegenelab:experimental_factors ?factors .\n  FILTER(?factors = 'compound')\n}\nLIMIT 50"
    },
    "differential_expression": {
      "description": "Find differentially expressed genes in an assay",
      "sparql": "SELECT ?gene ?symbol ?log2fc ?pvalue WHERE {\n  ?expr a biolink:GeneExpressionMixin ;\n        biolink:subject ?assay ;\n        biolink:object ?gene ;\n        spokegenelab:log2fc ?log2fc ;\n        spokegenelab:adj_p_value ?pvalue .\n  ?gene biolink:symbol ?symbol .\n  FILTER(?pvalue < 0.05)\n  FILTER(ABS(?log2fc) > 1.5)\n}\nORDER BY ?pvalue\nLIMIT 100"
    },
    "count_by_organism": {
      "description": "Count studies by organism",
      "sparql": "SELECT ?taxon (COUNT(DISTINCT ?study) as ?count) WHERE {\n  ?study a biolink:Study ;\n         biolink:in_taxon ?taxon .\n  FILTER(!REGEX(?taxon, '^[0-9]+$'))\n}\nGROUP BY ?taxon\nORDER BY DESC(?count)"
    }
  }
